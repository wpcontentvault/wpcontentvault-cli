{
    "name": "wpcontentvault/wpcontentvaultcli",
    "type": "project",
    "description": "The CLI tool that helps to manage Wordpress posts",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-libxml": "*",
        "illuminate/log": "^11.44.2",
        "illuminate/queue": "^11.44.2",
        "illuminate/routing": "^11.44.2",
        "josantonius/language-code": "^2.0",
        "laravel-zero/framework": "^11.36.1",
        "laravel/prompts": "^0.3.5",
        "league/commonmark": "^2.5",
        "league/html-to-markdown": "^5.1",
        "seriyyy95/losst-extension": "dev-main",
        "seriyyy95/wpajaxconnector-php": "dev-main",
        "spatie/regex": "^3.1",
        "symfony/finder": "^7.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.1",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.7",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse -c phpstan.neon"
        ],
        "test": [
            "application test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "bin": ["application"],
    "repositories": [
        {
            "type": "path",
            "url": "packages/wpajaxconnector-php",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "extensions/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
