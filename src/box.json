{
    "chmod": "0755",
    "map": [
        {
            "from": "packages/wpajaxconnector-php",
            "to": "vendor/wpajaxconnector-php"
        }
    ],
    "directories": [
        "app",
        "bootstrap",
        "config",
        "database",
        "vendor",
        "resources"
    ],
    "files": [
        "composer.json"
    ],
    "exclude-composer-files": false,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ],
    "output": "wpcontentvault-cli"
}
