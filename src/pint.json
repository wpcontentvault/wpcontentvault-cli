{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "no_useless_else": true,
        "array_push": true,
        "lowercase_keywords": true,
        "lowercase_static_reference": true,
        "ordered_interfaces": true,
        "ordered_traits": true,
        "global_namespace_import": {
            "import_classes": true
        },
        "php_unit_method_casing": {
            "case": "camel_case"
        },
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "destruct",
                "magic",
                "phpunit",
                "method_abstract",
                "method_public_static",
                "method_public",
                "method_protected_static",
                "method_protected",
                "method_private_static",
                "method_private"
            ]
        }
    }
}
